{
  "name": "inbox-zero",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "pretty-quick": "npx pretty-quick --write \"**/*.{ts,tsx}\"",
    "prettier": "npx prettier --write \"**/*.{ts,tsx}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "2.8.8",
    "pretty-quick": "^3.1.3",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.6.10",
  "lint-staged": {
    "*.{ts,tsx}": "pnpm run lint --cache --fix",
    "**/*.{ts,tsx}": "tsc-files --noEmit"
  }
}
